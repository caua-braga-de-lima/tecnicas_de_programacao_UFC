package exercicios;

public class ex03 
{
	public static void separacao(int vet[])
	{
		for(int i = 0; i < vet.length; i++)
		{
			for(int j = 1; j < vet.length; j++)
			{
				if((vet[i] < 0) && i != 0)
				{
					vet[i - j] = vet[i];
				}
			}
		}
	}
	public static void main(String[] args) 
	{
		

	}

}
